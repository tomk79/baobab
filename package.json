{
  "name": "Baobab",
  "version": "0.1.0-alpha1-nb",
  "description": "Baobab is the SystemFont Viewer Application.",
  "main": "index.html",
  "scripts": {
    "start": "nw",
    "build": "node ./build.js"
  },
  "author": "Tomoya Koyanagi",
  "license": "BSD",
  "dependencies": {
    "express": "^4.13.0",
    "glob": "^5.0.10",
    "mkdirp": "0.*",
    "phpjs": "^1.3.2",
    "font-manager": "^0.1.0",
    "socket.io": "^1.3.5"
  },
  "devDependencies": {
    "nw": "^0.12.2",
    "node-webkit-builder": "1.0.*",
    "gulp": "^3.8.10",
    "gulp-autoprefixer": "^2.3.1",
    "gulp-plumber": "^1.0.1",
    "gulp-sass": "^2.0.1",
    "gulp-uglify": "^1.2.0",
    "zip-folder": "^1.0.0"
  },
  "window": {
